.container.user-page
  %header.user-nav
    .media
      .media-left
        = link_to image_tag(@user.avatar,  size: "64x64", class: "media-object"), user_path(@user.id)
      .media-body
        %h4#top-aligned-media.media-heading
          = @user.name
          %span.label.label-default.project-number
            29
            %i Projects
        .proto-user
          %span.degree
            = @user.member
        .row
          .col-md-6
            %h4 Profile
            %p
              = @user.profile
          .col-md-6
            %h4 Works
            %p
              = @user.works
.container.proto-list
  .row
    = render partial: 'prototypes/prototype', collection: @prototypes

.text-center
  %ul.pagination
    %li.disabled
      = paginate(@prototypes)
